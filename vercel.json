{
    "version": 2,
    "builds": [
      {
        "src": "pages/api/vercel-api.ts",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri"
    },
    "routes": [
      {
        "src": "/api/vercel-api",
        "dest": "/pages/api/vercel-api.ts"
      }
    ]
  }
  